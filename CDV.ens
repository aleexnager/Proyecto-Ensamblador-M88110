;=======================MACROS========================

org 100

; MACROS

LEA: MACRO(ra, etiq)
	or ra, r0, low(etiq)
	or.u ra, ra, high(etiq)
ENDMACRO

LOAD: MACRO(ra, etiq)
	LEA(ra, etiq)
	ld ra, ra, r0
ENDMACRO

PUSH: MACRO(ra)
	sub r30, r30, 4
	st ra, r30, 0
ENDMACRO

POP: MACRO(ra)
	ld ra, r30, 0
	addu r30, r30, 4
ENDMACRO

